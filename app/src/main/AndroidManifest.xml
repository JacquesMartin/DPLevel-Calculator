<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vasoco.jacquesmartinvaso.DPLevelCalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.vasoco.jacquesmartinvaso.DPLevelCalculator.Impulse_Wet_Leg"
            android:windowSoftInputMode="adjustResize"/>
        <activity android:name="com.vasoco.jacquesmartinvaso.DPLevelCalculator.OpenTank_Capillary_Zero_Suppression"
