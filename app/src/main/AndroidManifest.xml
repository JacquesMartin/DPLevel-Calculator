        <activity android:name="com.vasoco.jacquesmartinvaso.DPLevelCalculator.OpenTank_Bubbler_System"
            android:windowSoftInputMode="adjustResize"/>
        <activity android:name="com.vasoco.jacquesmartinvaso.DPLevelCalculator.ClosedTank_Capillary_TwoSeal_System"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustResize"><intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" /></intent-filter></activity>
        <activity android:name="com.vasoco.jacquesmartinvaso.DPLevelCalculator.ClosedTank_Impulse_WetLeg"
            android:windowSoftInputMode="adjustResize"/>
        <activity android:name="com.vasoco.jacquesmartinvaso.DPLevelCalculator.ClosedTank_Impulse_DryLeg"
            android:windowSoftInputMode="adjustResize"/>
        <activity android:name="com.vasoco.jacquesmartinvaso.DPLevelCalculator.ClosedTank_Capillary_Interface"
            android:windowSoftInputMode="adjustResize"/>
